	RET
;----------------------------------------------------------------------
KR.VORSP:
	LD	DE,UE.BWSA+327H
	CALL	UC.CVDB
	LD	B,L
	LD	A,'F'
	LD	(UB.PAR2),A
	CALL	UR.KRE
	CALL	UR.POSIK
	RET
;---------------------------------------------------------------------
	DW	$
	DB	'XKA1'
	CALL	KR.GR1
	JR	$+11
;
