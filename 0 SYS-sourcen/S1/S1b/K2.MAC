	DB	'XKA2'
	CALL	KR.GR2
	CALL	KR.VORSP
	CALL	FD.FDINP
	CALL	UR.KRW
	CALL	UR.KRA
	RET
;----------------------------------------------------------------------
	DW	$
	DB	'YKA1'
	CALL	KR.GR1
	JR	$+11
;
	DW	$
	DB	'YKA2'
	CALL	KR.GR2
	CALL	UR.KRE
	LD	DE,UE.BWSA+327H
	CALL	UC.CVDB
	DEC	L
	JR	Z,$+11
	LD	(IX+10),L
	CALL	KR.TYPT
	CALL	UR.KLV
	CALL	FD.FDOUT
	RET
;---------------------------------------------------------------------------
	DW	$
	DB	'K1CL'
	CALL	KR.GR1
	JR	$+11
;
